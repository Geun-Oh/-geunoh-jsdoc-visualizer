{
  "name": "@geunoh/jsdoc-visualizer",
  "version": "1.1.3",
  "description": "JSDoc Visualizer for everyone. Overview your functions.",
  "bin": {
    "jv": "dist/cli.js"
  },
  "scripts": {
    "dev": "nodemon --watch \"./App.tsx\" --exec \"npm run render\"",
    "start": "node dist/cli.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "render": "esbuild ./index.tsx --outdir=public --bundle",
    "build": "esbuild ./cli.tsx --outdir=dist --bundle --platform=node"
  },
  "keywords": [
    "geunoh",
    "Geun-Oh",
    "JSDoc"
  ],
  "author": {
    "name": "Geun-Oh",
    "email": "kandy1002@naver.com",
    "url": "https://github.com/Geun-Oh"
  },
  "contributors": [
    "Geun-Oh"
  ],
  "homepage": "https://github.com/Geun-Oh/jsdoc-visualizer?tab=readme-ov-file",
  "private": false,
  "main": "dist/cli.js",
  "module": "dist/cli.js",
  "files": [
    "dist",
    "!lib",
    "!node_modules",
    "!src",
    "!App.tsx",
    "!cli.tsx",
    "!package-lock.json",
    "!tsconfig.json"
  ],
  "license": "ISC",
  "dependencies": {
    "commander": "^12.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/styled-components": "^5.1.34",
    "esbuild": "^0.20.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2"
  }
}
